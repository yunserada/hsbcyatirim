    <script>
        $.get("/api/hsbcdata/getGoldData").then(function (result, responseText, jqXHR) {
            if (jqXHR.status == 204) {
                HSBC.WarningCode204($(".goldData table"));
                return false;
            }
            $(result).each(function (e, i) {
                s = i.Symbol;
                if (i.Symbol == "USD/ONS") {
                    s = "XAUKO";
                }
                $("#" + s + "Buy").text(i.Buy.toString().money());
                $("#" + s + "Sell").text(i.Sell.toString().money());
            });
        });
    </script>




    <script>
        $("#ddlFX").on("change", function () {
            var url = "/api/hsbcdata/GetParityGraphData?code=" + $(this).val() + "&period=" + $(this).parents(".chart-wrapper").find(".chart-period-type a.active").data("period");
